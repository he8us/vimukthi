{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": "> 5%, last 2 versions"
        }
      }
    ],
    "flow",
    "react"
  ],
  "plugins": [
    [
      "transform-builtin-extend",
      {
        "globals": [
          "Error"
        ]
      }
    ],
    "syntax-class-properties",
    "transform-class-properties",
    "transform-object-rest-spread"
  ]
}
